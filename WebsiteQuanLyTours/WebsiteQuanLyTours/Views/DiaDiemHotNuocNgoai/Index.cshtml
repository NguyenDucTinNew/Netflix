        <th>Mã địa điểm hot nước ngoài</th>
        <th>Địa điểm hot nước ngoài</th>

    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.MaDiaDiemHotNuocNgoai</td>
            <td>@item.DiaDiemHotNuocNgoai1</td>
            <td><a href="/DiaDiemHotNuocNgoai/ChinhSua/@item.MaDiaDiemHotNuocNgoai">Edit</a></td>
            <td><a href="/DiaDiemHotNuocNgoai/Xoa/@item.MaDiaDiemHotNuocNgoai">Delete</a></td>
        </tr>
    }
</table>

